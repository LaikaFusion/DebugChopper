{
    "name": "debugchopper",
    "displayName": "debugchopper",
    "description": "Removes Console.logs and Debuggers from JS and JSX files",
    "version": "0.3.0",
    "publisher": "laikaf",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.debugchop"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.debugchop",
                "title": "Debug Chop"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
